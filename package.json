{
  "name": "ec-elgamal-circuit",
  "version": "2.0.0",
  "description": "Circom2 Circuit of ElGamal Scheme over Baby Jubjub Curve",
  "main": "index.js",
  "scripts": {
    "ptau": "./scripts/ptau.sh",
    "build-circuits": "./scripts/build-all-circuits.sh",
    "test": "ts-mocha --timeout 500000000 ./test/*.test.ts --exit",
    "test-elgamal": "ts-mocha --timeout 50000000 ./src/test/*.test.ts --quiet --exit",
    "delete-lookupTables": "rm ./lookupTables/*.json",
    "precompute-lookupTable": "yarn ts-node ./build/precompute.ts",
    "bench-negate": "snarkjs ri ./circuits/artifacts/negatePoint_test/negatePoint.r1cs",
    "bench-encrypt": "snarkjs ri ./circuits/artifacts/encrypt_test/encrypt.r1cs",
    "bench-decrypt": "snarkjs ri ./circuits/artifacts/decrypt_test/decrypt.r1cs",
    "benchmark-elgamal": "yarn bench-encrypt && yarn bench-decrypt",
    "benchmark": "yarn bench-negate && yarn benchmark-elgamal",
    "format": "yarn prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" -w",
    "lint": "yarn prettier \"src/**/*{ts,js}\" --check"
  },
  "keywords": [
    "ZKP",
    "circom2",
    "elgamal",
    "homomorphic",
    "baby jubjub"
  ],
  "author": "Shigoto-dev19",
  "license": "ISC",
  "devDependencies": {
    "@noble/curves": "^1.1.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.3",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.19",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "cli-progress": "^3.11.2",
    "ffjavascript": "^0.2.60",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "snarkjs": "^0.5.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
